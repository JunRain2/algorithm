n = int(input())
array = [list(input()) for _ in range(n)]
array.sort(key = lambda x : -len(x))

data = [-1] * 10

current_num = 9
tmp = []
for a in array:
    s = ''
    for j in a:
        index = ord(j) - ord('A')
        if data[index] == -1:
            data[index] = current_num
            current_num -= 1
        s += str(data[index])
    tmp.append(int(s))

print(data)
print(sum(tmp))